        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="ECEsitoPosNegType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="POSITIVO"/>
            <xs:enumeration value="NEGATIVO"/>
            <xs:enumeration value="NON_ATTIVATO"/>
        </xs:restriction>
    </xs:simpleType>
  
    <!--  -->
    <xs:simpleType name="ECEsitoPosNegWarType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="POSITIVO"/>
            <xs:enumeration value="NEGATIVO"/>
            <xs:enumeration value="WARNING"/>
            <xs:enumeration value="NON_ATTIVATO"/>
        </xs:restriction>
    </xs:simpleType>

    <!--  -->
    <xs:simpleType name="ECEsitoPosNegNesType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="POSITIVO"/>
            <xs:enumeration value="NEGATIVO"/>
            <xs:enumeration value="NON_ATTIVATO"/>
            <xs:enumeration value="NON_ESEGUITO"/>
        </xs:restriction>
    </xs:simpleType>

  
